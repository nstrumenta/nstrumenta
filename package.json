{
  "name": "nstrumenta",
  "version": "3.0.30",
  "description": "cli tool for nstrumenta",
  "main": "dist/nodejs/nodejs/index.js",
  "types": "dist/nodejs/nodejs/index.d.ts",
  "bin": {
    "nstrumenta": "dist/cli/index.js",
    "nst": "dist/cli/index.js"
  },
  "files": [
    "dist/**",
    "public/index.ejs",
    "public/**"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:agent-admin-page": "(cd agent-admin-page ; npm run build)",
    "build:cli": "tsc --project ./src/cli",
    "build:nodejs": "tsc --project ./src/nodejs",
    "build:browser": "tsc --project ./src/browser",
    "build": "npm run build:cli ; npm run build:nodejs ; npm run build:browser ; npm run build:agent-admin-page",
    "dev": "nodemon -- agent start --debug=true --tag=local",
    "format": "prettier -w ./",
    "release": "npm run clean; npm run build ; standard-version --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nstrumenta/nstrumenta.git"
  },
  "keywords": [
    "nstrumenta"
  ],
  "author": "Tyler Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nstrumenta/nstrumenta/issues"
  },
  "homepage": "https://github.com/nstrumenta/nstrumenta#readme",
  "devDependencies": {
    "@types/inquirer": "^8.1.1",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.7.21",
    "@types/semver": "^7.3.9",
    "@types/serve-index": "^1.9.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.0",
    "jest": "^29.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@fidm/x509": "^1.2.1",
    "@google-cloud/storage": "^5.16.1",
    "@mcap/core": "^0.2.2",
    "@types/aes-js": "^3.1.1",
    "@types/buffer-crc32": "^0.2.0",
    "@types/bytebuffer": "^5.0.42",
    "@types/debug": "^4.1.7",
    "@types/elliptic": "^6.4.14",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.186",
    "@types/tar": "^6.1.1",
    "aes-js": "^3.1.2",
    "airpc": "^0.3.1",
    "axios": "^0.26.1",
    "big-integer": "^1.6.51",
    "binary-data": "^0.6.0",
    "buffer-crc32": "^0.2.13",
    "bytebuffer": "^5.0.1",
    "commander": "^8.1.0",
    "conf": "^10.0.3",
    "debug": "^4.3.4",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "inquirer": "^8.1.5",
    "int64-buffer": "^1.0.1",
    "ip": "^1.1.8",
    "jspack": "^0.0.4",
    "lodash": "^4.17.21",
    "p-cancelable": "^2.0.0",
    "rx.mini": "^1.2.2",
    "semver": "^7.3.5",
    "serve-index": "^1.9.1",
    "tar": "^6.1.11",
    "turbo-crc32": "^1.0.1",
    "uuid": "^9.0.0",
    "werift": "^0.17.0",
    "ws": "^8.2.2"
