{
  "name": "nstrumenta",
  "version": "4.2.1",
  "description": "cli tool for nstrumenta",
  "main": "dist/agent/index.js",
  "types": "dist/agent/index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "dist/browser-esm/browser/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/browser-esm/browser/index.d.ts",
        "import": "./dist/browser-esm/browser/index.js"
      },
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.js"
    },
    "./browser": {
      "types": "./dist/browser-esm/browser/index.d.ts",
      "import": "./dist/browser-esm/browser/index.js"
    },
    "./cli/commands/module": {
      "types": "./dist/cli/commands/module.d.ts",
      "default": "./dist/cli/commands/module.js"
    }
  },
  "bin": {
    "nstrumenta": "dist/cli/index.js",
    "nst": "dist/cli/index.js"
  },
  "files": [
    "dist/**",
    "public/index.ejs",
    "public/**"
  ],
  "scripts": {
    "clean": "rm -rf dist dist-esm",
    "build:agent-admin-page": "(cd agent-admin-page ; npm i ; npm run build)",
    "build:lib": "tsc --project ./lib",
    "build:cli": "tsc --project ./cli",
    "build:browser": "tsc --project ./browser",
    "build:server": "(cd server/app ; rm -rf node_modules ; npm i ; npm run build)",
    "build:vscode-extension": "(cd vscode-extension ; rm -rf node_modules ; npm i ; npm run compile)",
    "build": "npm run build:lib && npm run build:cli && npm run build:browser && npm run build:agent-admin-page && npm run build:server && npm run build:vscode-extension",
    "build:all": "npm run build && npm run build:functions && npm run build:frontend",
    "build:functions": "(cd functions ; npm install ; npm run build)",
    "build:frontend": "(cd frontend ; npm install ; npm run build)",
    "watch": "nodemon --exec 'npm run build' --watch ./src",
    "watch:link": "nodemon --exec 'npm run build:browser && npm link' --watch ./src",
    "watch:vscode-extension": "tsc --watch --project ./src/vscode-extension",
    "dev": "nodemon -- module list",
    "dev:agent": "nodemon -- agent start",
    "dev:local.env": "dotenv -e ./credentials/local.env -- bash -c 'nodemon -- services list'",
    "start:agent": "npx node ./dist/cli/index.js agent start",
    "test": "npm run test:client && npm run test:server && npm run test:frontend",
    "test:client": "node --test --experimental-strip-types cli/__tests__/**/*.test.ts agent/__tests__/**/*.test.ts lib/__tests__/**/*.test.ts",
    "test:server": "cd server/app && npm install && npm test",
    "test:frontend": "cd frontend && npm install && npm test",
    "test:cli": "NSTRUMENTA_API_KEY=test:aHR0cDovL2xvY2FsaG9zdDo4MDgw ts-node cli/__tests__/mcp.test.ts",
    "test:watch": "node --test --watch --experimental-strip-types agent/__tests__/**/*.test.ts",
    "test:coverage": "node --test --experimental-test-coverage --experimental-strip-types agent/__tests__/**/*.test.ts",
    "test:vscode-extension": "node --test --experimental-strip-types vscode-extension/__tests__/unit/**/*.test.ts",
    "test:all": "npm run test:client && npm run test:server && npm run test:frontend && npm run test:vscode-extension",
    "test:e2e": "./integration-tests/e2e.sh",
    "test:e2e:quick": "rm -rf build/*.tgz && ./integration-tests/e2e.sh",
    "profile": "npm run build:cli ; npx 0x --output-dir profile ./dist/cli/index.js agent start --tag=local",
    "format": "prettier -w ./",
    "release": "npm run clean && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nstrumenta/nstrumenta.git"
  },
  "keywords": [
    "nstrumenta"
  ],
  "author": "Tyler Bryant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nstrumenta/nstrumenta/issues"
  },
  "homepage": "https://github.com/nstrumenta/nstrumenta#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.2",
    "@types/inquirer": "^8.1.1",
    "@types/node": "^22.13.0",
    "@types/semver": "^7.3.9",
    "@types/serve-index": "^1.9.1",
    "@types/tar": "^6.1.5",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.2.0",
    "dotenv-cli": "^7.2.1",
    "nodemon": "^3.1.4",
    "prettier": "^2.4.1",
    "release-it": "^19.0.6",
    "ts-node": "^10.2.1",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@mcap/core": "^0.2.2",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@types/bytebuffer": "^5.0.42",
    "bytebuffer": "^5.0.1",
    "commander": "^8.1.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "inquirer": "^8.1.5",
    "semver": "^7.5.4",
    "serve-index": "^1.9.1",
    "tar": "^6.1.15",
    "uuid": "^9.0.0",
    "ws": "^8.18.0",
    "zod": "^4.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
