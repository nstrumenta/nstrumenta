{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": [
      "ESNext", "ES2020"
    ],
    "declaration": true,
    "outDir": "../../dist",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false
  },
}