{
  "name": "nst-server",
  "version": "2.0.0",
  "description": "server for nstrumenta",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc -p .",
    "typecheck": "npx tsc --noEmit -p .",
    "format": "prettier -w ./src",
    "dev-dotenv": "dotenv -e $ENVFILE -- bash -c 'env && npm run dev'",
    "dev": "tsx watch --inspect=0.0.0.0:9229 src/index.ts"
  },
  "author": "Tyler Bryant",
  "license": "UNLICENSED",
  "dependencies": {
    "@google-cloud/compute": "^4.7.0",
    "@google-cloud/firestore": "^7.9.0",
    "@google-cloud/storage": "^7.12.0",
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "firebase-admin": "^13.4.0",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.4",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.2",
    "prettier": "^2.2.1",
    "tsx": "^4.20.6",
    "typescript": "^4.4.2"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "preferTemplate": true
  }
}
