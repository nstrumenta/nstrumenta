{
  "name": "nstrumenta-vscode",
  "displayName": "nstrumenta",
  "description": "Integration with https://nstrumenta.com",
  "version": "4.0.0",
  "publisher": "nstrumenta",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/nstrumenta/nstrumenta.git",
    "directory": "src/vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/nstrumenta/nstrumenta/issues"
  },
  "homepage": "https://nstrumenta.com",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "nstrumenta",
    "sensor",
    "data",
    "iot",
    "mcp"
  ],
  "icon": "favicon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nstrumenta.setApiKey",
        "title": "Set API Key",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.clearApiKey",
        "title": "Clear API Key",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.status",
        "title": "Show Status",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.module.list",
        "title": "List Modules",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.module.run",
        "title": "Run Module",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.module.publish",
        "title": "Publish Modules",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.data.list",
        "title": "List Data",
        "category": "Nstrumenta"
      },
      {
        "command": "nstrumenta.agent.list",
        "title": "List Agents",
        "category": "Nstrumenta"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node --test --experimental-strip-types __tests__/unit/**/*.test.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2"
  }
}
