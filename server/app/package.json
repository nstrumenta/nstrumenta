{
  "name": "nst-server",
  "version": "4.1.0",
  "description": "server for nstrumenta",
  "bin": {
    "nst-server": "dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc -p .",
    "typecheck": "npx tsc --noEmit -p .",
    "format": "prettier -w ./src",
    "dev-dotenv": "dotenv -e $ENVFILE -- bash -c 'env && npm run dev'",
    "dev": "tsx watch --inspect=0.0.0.0:9229 src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Tyler Bryant",
  "license": "UNLICENSED",
  "dependencies": {
    "@google-cloud/compute": "^4.7.0",
    "@google-cloud/firestore": "^7.9.0",
    "@google-cloud/storage": "^7.12.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.4.0",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.4",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^22.13.0",
    "@types/uuid": "^9.0.2",
    "prettier": "^2.2.1",
    "tsx": "^4.20.6",
    "typescript": "^5.6.0",
    "vitest": "^4.0.9"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "preferTemplate": true
  }
}
